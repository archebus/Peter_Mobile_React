{
  "border": {
    "borderWidth": 1,
    "borderColor": "#000",
    "padding": 10,
    "margin": 10,
    "backgroundColor": "#FF69B4"
  },
  "button": {
    "backgroundColor": "#d6213b",
    "padding": 10,
    "margin": 5,
    "borderRadius": 5
  },
  "buttonText": {
    "color": "#FFF",
    "textAlign": "center"
  },
  "buttonContainer": {
    "flexDirection": "row",
    "justifyContent": "space-around",
    "alignItems": "center",
    "marginBottom": 20
  },
  "buttonRow": {
    "flexDirection": "row",
    "justifyContent": "space-between",
    "alignItems": "center",
    "marginTop": 10
  },
  "userContainer": {
    "padding": 10,
    "margin": 10,
    "borderWidth": 1,
    "borderColor": "#fff"
  },
  "imageStyle": {
    "width": "100%",
    "aspectRatio": 1,
    "resizeMode": "contain"
  }
}
